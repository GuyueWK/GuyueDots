        },
        "group/cava": {
            "orientation": "horizontal",
            "modules": [
                "custom/cava"
            ]
        },
        "custom/cava": {
            "format": "[{}]",
            "exec": "~/.config/waybar/scripts/cava_smart.sh",
            "return-type": "json",
            "interval": 2,
            "exec-if": "command -v cava >/dev/null 2>&1",
            "tooltip": false,
            "max-length": 30
        },
        "tray": {
            "icon-size": 14,
            "spacing": 10
        },
        "custom/notifications": {
            "format": "ÔÉ≥",
            "tooltip": false,
            "on-click": "~/.config/waybar/scripts/notification_center.sh"
        }
    }
]